{
  "name": "monorepo",
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@9.15.0",
  "private": true,
  "scripts": {
    "build": "pwsh scripts/Build-Wasm.ps1 && pnpm run -F find-a-plugin build",
    "check": "pnpm run -F find-a-plugin check",
    "check:watch": "pnpm run -F find-a-plugin check:watch",
    "clippy": "cargo clippy --all",
    "dev": "pnpm run -F find-a-plugin dev",
    "eslint": "eslint . --config eslint.config.js --cache",
    "format": "prettier . --write && cargo fmt --all",
    "update": "miho update major -t"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.13.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@tb-dev/eslint-config": "^5.6.0",
    "@tb-dev/utils": "^4.1.0",
    "@types/node": "^22.10.2",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^1.0.0-next.74",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.17.0",
    "lucide-svelte": "^0.468.0",
    "mode-watcher": "^0.5.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-svelte": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "svelte": "^5.14.6",
    "svelte-check": "^4.1.1",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-plugin-wasm": "~3.3.0"
  }
